{

  "compilerOptions": {
 
    "rootDir": "./src",
    "outDir": "./dist",
 
    "types": [],
  
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
 
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,

    "strict": true,
    "moduleResolution": "node",
    "target": "ES2023",
    "isolatedModules": true,
    "noUncheckedSideEffectImports": true,
    "moduleDetection": "force",
    "skipLibCheck": true,
    "esModuleInterop": true,
  },
"include": ["src"],

  "exclude": [
    "node_modules",
    "dist",
    "generated/prisma"
  ]
}